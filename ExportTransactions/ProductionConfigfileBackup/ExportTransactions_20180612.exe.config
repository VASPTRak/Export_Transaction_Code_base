<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	<connectionStrings>
		<!--<add name="FluidSecureConnectionString" connectionString="Data Source=103.8.126.241;Initial Catalog=FluidSecure_Local;uid=sa;pwd=cisco@123;MultipleActiveResultSets=True;Max Pool Size=75;Min Pool Size=1;Connect Timeout=60"
         providerName="System.Data.SqlClient" />
		<add name="FluidSecureConnectionString" connectionString="Data Source=103.8.126.241;Initial Catalog=FuelSecure;uid=sa;pwd=sql@123;MultipleActiveResultSets=True;Max Pool Size=75;Min Pool Size=1;Connect Timeout=60"
			 providerName="System.Data.SqlClient" />-->
<add name="FluidSecureConnectionString" connectionString="Data Source=FluidSecure\SQLEXPRESS;Initial Catalog=FluidSecure;uid=sa;pwd=Tr@k123#$;MultipleActiveResultSets=True;Max Pool Size=75;Min Pool Size=1;Connect Timeout=60" 
			 providerName="System.Data.SqlClient" />
	</connectionStrings>
	<appSettings>
		<add key="PathForExportTransactionEmailTemplate" value="C:\Program Files (x86)\Microsoft\ExportTransactions_SetUp\ExportTransaction.txt" />
		<add key="TransSubject" value="FluidSecure Export" />
		<add key="emailAccount" value="info@fluidsecure.com" />
		<add key="emailPassword" value="SimplySmart" />
		<add key="FromEmail" value="info@fluidsecure.com" />
		<add key="smtpPort" value="25" />
		<add key="smtpServer" value="smtpout.secureserver.net" />
		<add key="EnableSsl" value="False" />
	</appSettings>
	<log4net debug="true">
		<!--User below code for Disable log report-->
		<!--<log4net debug="true" threshold="OFF">-->
		<appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="LogReport.log" />
			<appendToFile value="true" />
			<rollingStyle value="Size" />
			<maxSizeRollBackups value="100" />
			<maximumFileSize value="10MB" />
			<staticLogFileName value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%-5p %d %5rms %-22.22c{1} %-18.18M - %m%n" />
			</layout>
		</appender>
		<root>
			<level value="DEBUG" />
			<appender-ref ref="RollingLogFileAppender" />
		</root>
	</log4net>
</configuration>